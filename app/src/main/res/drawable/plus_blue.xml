<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        >
        <shape>
            <stroke
                android:width="1dp"
                android:color="#19000000" />
            <corners android:radius="18dp" />
        </shape>
    </item>
    <item
        android:left="@dimen/_6dp"
        android:right="@dimen/_6dp">
        <shape android:shape="line">
            <stroke
                android:width="@dimen/_1dp"
                android:color="#4aa1ff" />
        </shape>
    </item>
    <item
        android:left="@dimen/_6dp"
        android:right="@dimen/_6dp">
        <rotate
            android:fromDegrees="90"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="-90">
            <shape android:shape="line">
                <stroke
                    android:width="@dimen/_1dp"
                    android:color="#4aa1ff" />
            </shape>
        </rotate>
    </item>

</layer-list>